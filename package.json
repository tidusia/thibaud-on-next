{
  "name": "thibaud-on-next",
  "description": "Personal website on Next.js",
  "scripts": {
    "build": "next build --no-lint",
    "deploy": "npm run build && npm run export",
    "dev": "next dev",
    "export": "next export",
    "formatters": "npm run prettier:check && npm run lint && npm run typescript:check",
    "integrity": "npm run formatters && npm run build && npm run storybook:build && npm run jest:all && npm run storybook:test:integrity",
    "jest:all": "jest",
    "jest:watch": "jest --watch",
    "lint": "next lint -- --ignore-path .gitignore",
    "pre-commit": "lint-staged --verbose && npm run typescript:check && jest -o && npm run storybook:test:only-changed",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:test": "test-storybook --url http://localhost:6006 --browsers chromium",
    "storybook:test:integrity": "start-server-and-test storybook:dev 6006 storybook:test",
    "storybook:test:only-changed": "start-server-and-test storybook:dev 6006 'npm run storybook:test -- -- -o'",
    "typescript:check": "tsc --noEmit"
  },
  "author": "@tidusia",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@mapbox/rehype-prism": "^0.9.0",
    "@vercel/analytics": "^1.2.2",
    "clsx": "^2.1.0",
    "gray-matter": "^4.0.3",
    "next": "^14.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-actions": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/nextjs": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@storybook/test-runner": "^0.17.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.72",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.2.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "start-server-and-test": "^2.0.3",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.3"
  }
}
