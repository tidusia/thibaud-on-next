{
  "name": "thibaud-on-next",
  "description": "Personal website on Next.js",
  "scripts": {
    "build": "next build --no-lint",
    "deploy": "npm run build && npm run export",
    "dev": "next dev",
    "export": "next export",
    "formatters": "npm run prettier:check && npm run lint && npm run typescript:check",
    "integrity": "npm run formatters && npm run build && npm run storybook:build && npm run jest:all && npm run storybook:test:integrity",
    "jest:all": "jest",
    "jest:watch": "jest --watch",
    "lint": "next lint -- --ignore-path .gitignore",
    "pre-commit": "lint-staged --verbose && npm run typescript:check && jest -o && npm run storybook:test:only-changed",
    "prepare": "husky install",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:test": "test-storybook --url http://localhost:6006 --browsers chromium",
    "storybook:test:integrity": "start-server-and-test storybook:dev 6006 storybook:test",
    "storybook:test:only-changed": "start-server-and-test storybook:dev 6006 'npm run storybook:test -- -- -o'",
    "typescript:check": "tsc --noEmit"
  },
  "author": "@tidusia",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@mapbox/rehype-prism": "^0.8.0",
    "clsx": "^1.2.1",
    "gray-matter": "^4.0.3",
    "next": "^12.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@storybook/addon-a11y": "^7.0.24",
    "@storybook/addon-actions": "^7.0.24",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/jest": "^0.1.0",
    "@storybook/nextjs": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tidusia/prettier-config": "^2.1.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.18",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "eslint": "^8.43.0",
    "eslint-config-next": "^12.3.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "lint-staged": "^12.5.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.0.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5"
  }
}
