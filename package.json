{
  "name": "thibaud-on-next",
  "description": "Personal website on Next.js",
  "engines": {
    "node": "^16",
    "npm": "^8"
  },
  "scripts": {
    "build": "next build",
    "deploy": "npm run build && npm run export",
    "dev": "next dev",
    "export": "next export",
    "integrity": "npm run prettier:check && npm run typescript:check && npm run deploy && npm run storybook:build",
    "lint": "next lint",
    "pre-commit": "lint-staged --verbose && npm run lint && npm run typescript:check && npm run test -- -o",
    "prepare": "husky install",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "typescript:check": "tsc --noEmit"
  },
  "author": "@tidusia",
  "license": "MIT",
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@tailwindcss/forms": "^0.3.4",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "autoprefixer": "^10.4.0",
    "gray-matter": "^4.0.3",
    "next": "^12.0.2",
    "postcss": "^8.3.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.0",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-interactions": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.9",
    "@storybook/testing-library": "^0.0.7",
    "@storybook/testing-react": "^1.2.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@tidusia/prettier-config": "^2.1.0",
    "@types/jest": "^27.0.2",
    "@types/jest-axe": "^3.5.3",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest-dom": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.5",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "jest-axe": "^5.0.1",
    "lint-staged": "^12.1.4",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
