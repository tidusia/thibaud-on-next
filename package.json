{
  "name": "thibaud-on-next",
  "description": "Personal website on Next.js",
  "scripts": {
    "build": "next build",
    "deploy": "npm run build && npm run export",
    "dev": "next dev",
    "export": "next export",
    "integrity": "npm run prettier:check && npm run typescript:check && npm run test && npm run deploy && npm run storybook:build",
    "lint": "next lint",
    "pre-commit": "lint-staged --verbose && npm run lint && npm run typescript:check && npm run test -- -o && npm run test:storybook",
    "prepare": "husky install",
    "prettier:check": "prettier . --check",
    "prettier:write": "prettier . --write",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "jest",
    "test:storybook": "test-storybook",
    "test:storybook:watch": "test-storybook --watch",
    "typescript:check": "tsc --noEmit"
  },
  "author": "@tidusia",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@mapbox/rehype-prism": "^0.8.0",
    "clsx": "^1.2.1",
    "gray-matter": "^4.0.3",
    "next": "^12.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@storybook/addon-a11y": "^7.0.20",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/jest": "^0.1.0",
    "@storybook/nextjs": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/testing-react": "^2.0.0",
    "@tailwindcss/forms": "^0.5.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@tidusia/prettier-config": "^2.1.0",
    "@types/jest": "^27.4.1",
    "@types/jest-axe": "^3.5.3",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "eslint": "^8.10.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-axe": "^6.0.0",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "storybook": "^7.0.20",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  }
